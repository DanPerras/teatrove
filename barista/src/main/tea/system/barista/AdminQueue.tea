        <td><font face="Arial" size="2"><b>Total Queue Duration (ms)</b></font> <A HREF="javascript:void(0)"
            ONMOUSEOVER="doTooltip(event,18,'<%desc["totalQueueDuration"]%>')"
            ONMOUSEOUT="hideTip()"><font size = -2>[info]</font></a></td>
        <td><font face="Arial" size="2"><%stats.totalQueueDuration%></font></td>
    </tr>
    <tr>
        <td><font face="Arial" size="2"><b>Total Service Duration (ms)</b></font> <A HREF="javascript:void(0)"
            ONMOUSEOVER="doTooltip(event,19,'<%desc["totalServiceDuration"]%>')"
            ONMOUSEOUT="hideTip()"><font size = -2>[info]</font></a></td>
        <td><font face="Arial" size="2"><%stats.totalServiceDuration%></font></td>
    </tr>
    <tr>
        <td><font face="Arial" size="2"><b>Snapshot Duration (ms)</b></font> <A HREF="javascript:void(0)"
            ONMOUSEOVER="doTooltip(event,20,'<%desc["snapshotDuration"]%>')"
            ONMOUSEOUT="hideTip()"><font size = -2>[info]</font></a></td>
        <td><font face="Arial" size="2"><%stats.snapshotDuration%> (<%call DisplayDuration(stats.snapshotDuration)%>)</font></td>
    </tr>
    <% numberFormat("0.00") %>
    <tr>
        <td><font face="Arial" size="2"><b>Average Queue Duration (ms)</b></font> <A HREF="javascript:void(0)"
            ONMOUSEOVER="doTooltip(event,21,'<%desc["averageQueueDuration"]%>')"
            ONMOUSEOUT="hideTip()"><font size = -2>[info]</font></a></td>
        <td><font face="Arial" size="2"><%stats.averageQueueDuration%></font></td>
    </tr>
    <tr>
        <td><font face="Arial" size="2"><b>Average Service Duration (ms)</b></font> <A HREF="javascript:void(0)"
            ONMOUSEOVER="doTooltip(event,22,'<%desc["averageServiceDuration"]%>')"
            ONMOUSEOUT="hideTip()"><font size = -2>[info]</font></a></td>
        <td><font face="Arial" size="2"><%stats.averageServiceDuration%></font></td>
    </tr>
    <% numberFormat("0.0000") %>
    <tr>
        <td><font face="Arial" size="2"><b>Enqueue Attempt Rate (#/s)</b></font> <A HREF="javascript:void(0)"
