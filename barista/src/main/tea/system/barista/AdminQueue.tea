<% template AdminQueue(org.teatrove.trove.util.tq.TransactionQueueData stats)

	queue = stats.transactionQueue
    desc = call TQDescriptions()
    numberFormat(null)%>
    <table border="1" cellpadding="2" cellspacing="1">
    <tr>
        <td bgcolor="#737398" colspan="2"><font size="2" face="Arial" color="#FFFFFF"><b><%queue.name%> 
        Transaction Queue</b></font>&nbsp;</td>
    </tr>
        <tr>
        <td>
          <DIV ID="tooltip" STYLE="position:absolute; visibility:hidden; z-index:10"></DIV>
          <font face="Arial" size="2"><b>Maximum Allowed Size</b></font> <A HREF="javascript:void(0)"
            ONMOUSEOVER="doTooltip(event,1,'<%desc["maximumSize"]%>')"
            ONMOUSEOUT="hideTip()"><font size = -2>[info]</font></a></td>
        <td><font face="Arial" size="2"><%queue.maximumSize%></font></td>
    </tr>
    <tr>
        <td>
            <font face="Arial" size="2"><b>Maximum Allowed Threads</b></font> <A HREF="javascript:void(0)"
            ONMOUSEOVER="doTooltip(event,2,'<%desc["maximumThreads"]%>')"
            ONMOUSEOUT="hideTip()"><font size = -2>[info]</font></a></td>
        <td><font face="Arial" size="2"><%queue.maximumThreads%></font></td>
    </tr>
    <tr>
        <td><font face="Arial" size="2"><b>Snapshot Start</b></font> <A HREF="javascript:void(0)"
            ONMOUSEOVER="doTooltip(event,3,'<%desc["snapshotStart"]%>')"
            ONMOUSEOUT="hideTip()"><font size = -2>[info]</font></a></td>
        <td><font face="Arial" size="2"><%stats.snapshotStart%></font></td>
    </tr>
    <tr>
        <td><font face="Arial" size="2"><b>Snapshot End</b></font> <A HREF="javascript:void(0)"
            ONMOUSEOVER="doTooltip(event,4,'<%desc["snapshotEnd"]%>')"
            ONMOUSEOUT="hideTip()"><font size = -2>[info]</font></a></td>
        <td><font face="Arial" size="2"><%stats.snapshotEnd%></font></td>
    </tr>
    <tr>
        <td><font face="Arial" size="2"><b>Queue Size</b></font> <A HREF="javascript:void(0)"
            ONMOUSEOVER="doTooltip(event,5,'<%desc["queueSize"]%>')"
            ONMOUSEOUT="hideTip()"><font size = -2>[info]</font></a></td>
        <td><font face="Arial" size="2"><%stats.queueSize%></font></td>
    </tr>
    <tr>
        <td><font face="Arial" size="2"><b>Thread Count</b></font> <A HREF="javascript:void(0)"
            ONMOUSEOVER="doTooltip(event,6,'<%desc["threadCount"]%>')"
            ONMOUSEOUT="hideTip()"><font size = -2>[info]</font></a></td>
        <td><font face="Arial" size="2"><%stats.threadCount%></font></td>
    </tr>
    <tr>
        <td><font face="Arial" size="2"><b>Servicing Count</b></font> <A HREF="javascript:void(0)"
            ONMOUSEOVER="doTooltip(event,7,'<%desc["servicingCount"]%>')"
            ONMOUSEOUT="hideTip()"><font size = -2>[info]</font></a></td>
        <td><font face="Arial" size="2"><%stats.servicingCount%></font></td>
    </tr>
    <tr>
        <td><font face="Arial" size="2"><b>Peak Queue Size</b></font> <A HREF="javascript:void(0)"
            ONMOUSEOVER="doTooltip(event,8,'<%desc["peakQueueSize"]%>')"
            ONMOUSEOUT="hideTip()"><font size = -2>[info]</font></a></td>
        <td><font face="Arial" size="2"><%stats.peakQueueSize%></font></td>
    </tr>
    <tr>
        <td><font face="Arial" size="2"><b>Peak Thread Count</b></font> <A HREF="javascript:void(0)"
            ONMOUSEOVER="doTooltip(event,9,'<%desc["peakThreadCount"]%>')"
            ONMOUSEOUT="hideTip()"><font size = -2>[info]</font></a></td>
        <td><font face="Arial" size="2"><%stats.peakThreadCount%></font></td>
    </tr>
    <tr>
        <td><font face="Arial" size="2"><b>Peak Servicing Count</b></font> <A HREF="javascript:void(0)"
